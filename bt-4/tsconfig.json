{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "rootDir": ".",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "functions/**/*.ts",
    "types/**/*.d.ts",
    "index.ts",
    "compress.ts",
    "decompress.ts"
  ]
}


